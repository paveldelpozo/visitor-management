        "npm-run-all": "^4.1.5",
        "pusher-js": "^8.4.0",
        "typescript": "^5.3.3",
        "vite": "^5.1.0",
        "vue-tsc": "^1.8.21"
    },
    "dependencies": {
        "@mdi/font": "^7.4.47",
        "axios": "^1.6.0",
        "pinia": "^2.1.7",
        "vue": "^3.4.15",
        "vue-router": "^4.2.5",
        "vuetify": "^3.6.0"
