{
    "name": "seonthebeach-visitor-management",
    "type": "module",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:vite": "vite",
        "local-ip": "ip a | grep 'inet '",
